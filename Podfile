# platform :ios, '9.0'

target 'MultiplatformMessenger (iOS)' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for MultiplatformMessenger (iOS)
  pod 'CocoaMQTT'
